####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS) $(OBJCOMP)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: oggeditor.pro  /usr/lib/qt-3.2.1/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile oggeditor.pro
qmake: 
	@$(QMAKE) -o Makefile oggeditor.pro

dist:
	@mkdir -p .obj/oggeditor && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/oggeditor/ && ( cd `dirname .obj/oggeditor` && $(TAR) oggeditor.tar oggeditor && $(GZIP) oggeditor.tar ) && $(MOVE) `dirname .obj/oggeditor`/oggeditor.tar.gz . && $(DEL_FILE) -r .obj/oggeditor

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


