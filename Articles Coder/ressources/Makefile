.obj/main.o: main.cpp oggeditordialog.h \
		.ui/MainForm.h \
		OggLib/OggFile.h \
		OggLib/vcedit.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.obj/oggeditordialog.o: oggeditordialog.cpp oggeditordialog.h \
		.ui/MainForm.h \
		OggLib/OggFile.h \
		OggLib/vcedit.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/oggeditordialog.o oggeditordialog.cpp

.obj/OggFile.o: OggLib/OggFile.cpp OggLib/OggFile.h \
		OggLib/vcedit.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/OggFile.o OggLib/OggFile.cpp

.obj/vcedit.o: OggLib/vcedit.c OggLib/vcedit.h \
		OggLib/i18n.h
	$(CC) -c $(CFLAGS) $(INCPATH) -o .obj/vcedit.o OggLib/vcedit.c

.ui/MainForm.h: MainForm.ui
	$(UIC) MainForm.ui -o .ui/MainForm.h

.ui/MainForm.cpp: .ui/MainForm.h MainForm.ui 
	$(UIC) MainForm.ui -i MainForm.h -o .ui/MainForm.cpp

.obj/MainForm.o: .ui/MainForm.cpp .ui/MainForm.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/MainForm.o .ui/MainForm.cpp

.obj/moc_oggeditordialog.o: .moc/moc_oggeditordialog.cpp oggeditordialog.h .ui/MainForm.h \
		OggLib/OggFile.h \
		OggLib/vcedit.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_oggeditordialog.o .moc/moc_oggeditordialog.cpp

.obj/moc_MainForm.o: .moc/moc_MainForm.cpp .ui/MainForm.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_MainForm.o .moc/moc_MainForm.cpp

.moc/moc_oggeditordialog.cpp: $(MOC) oggeditordialog.h
	$(MOC) oggeditordialog.h -o .moc/moc_oggeditordialog.cpp

.moc/moc_MainForm.cpp: $(MOC) .ui/MainForm.h
	$(MOC) .ui/MainForm.h -o .moc/moc_MainForm.cpp

